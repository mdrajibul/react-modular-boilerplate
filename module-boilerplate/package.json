{
    "name": "@mdrajibul/react-module-boilerplate",
    "moduleName": "web-module-boilerplate",
    "version": "1.0.0",
    "description": "A reactjs module boilerplate",
    "main": "dist/index.js",
    "scripts": {
        "start": "webpack-dev-server --color --config ./webpack/config.js --profile=dev --open --history-api-fallback",
        "build": "tsc && npm run copy:assets",
        "build:prod": "del-cli dist && webpack --config ./webpack/config.js --profile=dev --env.environment=production",
        "copy:assets": "cpy ./ ../../dist/assets --cwd=src/assets --parents",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --watchAll=false",
        "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
        "lint": "eslint --ext .ts,.tsx,.jsx,.js src/**",
        "lint:fix": "eslint --ext .ts,.tsx,.jsx,.js src/** --fix",
        "preCommit": "npm run prettier && npm run lint && npm run test:coverage && npm run build"
    },
    "author": "Md Rajib ul Islam <mdrajibul@gmail.com>",
    "license": "ISC",
    "files": [
        "dist/",
        "index.js",
        "index.d.ts"
    ],
    "peerDependencies": {
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "styled-components": "^5.2.1",
        "axios": "^0.21.0"
    },
    "devDependencies": {
        "@mdrajibul/cloud-config-utils": "^1.0.0",
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^11.2.2",
        "@testing-library/react-hooks": "^3.4.2",
        "@types/eslint": "^7.2.0",
        "@types/jest": "^26.0.15",
        "@types/node": "12.11.1",
        "@types/react": "^16.14.1",
        "@types/react-dom": "^16.9.10",
        "@types/react-router-dom": "^5.1.6",
        "@types/styled-components": "^5.1.4",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "@typescript-eslint/parser": "3.7.0",
        "axios": "^0.21.0",
        "copy-webpack-plugin": "5.0.4",
        "cors": "2.8.5",
        "cpy-cli": "^3.1.1",
        "css-loader": "^5.0.1",
        "del-cli": "^3.0.1",
        "dotenv": "8.2.0",
        "eslint": "7.5.0",
        "eslint-plugin-react": "7.20.3",
        "file-loader": "6.0.0",
        "history": "4.10.1",
        "html-webpack-plugin": "4.3.0",
        "jest": "^26.6.3",
        "jest-styled-components": "^7.0.3",
        "prettier": "2.0.5",
        "pretty-quick": "2.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "react-test-renderer": "^16.13.1",
        "source-map-loader": "1.0.1",
        "style-loader": "^2.0.0",
        "styled-components": "^5.2.1",
        "ts-jest": "^26.1.1",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "url-loader": "4.1.0",
        "webpack": "^5.4.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    }
}